using System;

namespace CLT
{
    class Program
    {
        static void Main(string[] args)
        {
            var weight = 9800.0;
            var mean = 205;
            var stDev = 15;
            var n = 49;

            var possibleWeight = mean * n;
            var probability = weight / possibleWeight;
            Console.WriteLine("{0:0.0000}", probability / 100.0);
        }
    }
}
